var edited_html;
var edited_id;

var productLink = $('#new_product_link');
shuffleLinks(productLink);

$('#product_list li:last-child').append('<%= j render(@product) %>');

edited_id = $('#product_list li:last').attr('id');
edited_html = $('#product_list li:last').html();
$('li#' + edited_id + ':first').replaceWith(edited_html);
$('#product_list li:last').remove();

